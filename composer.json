{
    "name": "kevinlabre/forest-admin-schema",
    "authors": [
        {
            "name": "Kevin Labre",
            "email": "kevinlabre@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/support": "^5.5|^6|^7|^8",
        "illuminate/http": "^5.5|^6|^7|^8",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "orchestra/testbench": "6.x-dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "KevinLbr\\ForestAdminSchema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KevinLbr\\ForestAdminSchema\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KevinLbr\\ForestAdminSchema\\Providers\\SchemaServiceProvider"
            ]
        }
    }
}
